{
  "name": "gpt-auto-committer",
  "version": "1.0.22",
  "description": "CLI tool for automating commits & GitHub pull requests using LLMs",
  "bin": {
    "gac": "build/index.js"
  },
  "postinstall": "node postinstall.js",
  "keywords": [
    "git",
    "chatgpt",
    "gpt",
    "ai",
    "openai",
    "aicommit",
    "aicommits",
    "gptcommit",
    "commit",
    "pullrequest",
    "github",
    "jira"
  ],
  "preferGlobal": true,
  "scripts": {
    "prepublish": "npm run build",
    "start": "ts-node src/index.ts",
    "build": "tsc"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "handlebars": "^4.7.7",
    "node-fetch": "^2.6.10",
    "openai": "^4.24.0",
    "ts-node": "^10.9.2"
  }
}
